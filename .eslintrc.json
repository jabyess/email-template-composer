{
    "env": {
        "browser": true,
        "node" : true,
        "es6" : true
    },
    "parserOptions" : {
      "parser" : "babel-eslint",
      "sourceType" : "module",
      "ecmaVersion" : 6,
      "ecmaFeatures" : {
        "jsx" : true,
        "modules" : true
      }
    },
    "extends": "eslint:recommended",
    "rules" : {
      "no-unused-vars" : "off",
      "no-console" : "off"
    }
}
